namespace API\Campus\Entity;

/**
 * Class Localisation
 * @package src\API\Campus\Entity
 * @Entity(repositoryClass="API\Campus\Entity\LocalisationRepository")
 */
class Localisation {

    /**
     * @Id @Column(type="integer")
     * @GeneratedValue
     */
    private $id;

    /**
     * @Column(type="float")
     */
    private $lat;

    /**
     * @Column(type="float")
     */
    private $lng;
}