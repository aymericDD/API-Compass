namespace API\Campus\Entity;

/**
 * Class Address
 * @package src\API\Campus\Entity
 * @Entity(repositoryClass="API\Campus\Entity\AddressRepository")
 */
class Address {
    /**
     * @Id @Column(type="integer")
     * @GeneratedValue
     */
    private $id;

    /** @Column(type="text") */
    private $rue;

    /** @Column(type="integer") */
    private $codepostale;

    /** @Column(type="text") */
    private $ville;
}